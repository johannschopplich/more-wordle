{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:types": "vue-tsc --noEmit",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{css,html,json,md,ts,vue,yml}\" --ignore-path .gitignore",
    "prepare": "simple-git-hooks"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --cache --ignore-path .gitignore"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@leanera/vue-i18n": "^0.5.0",
    "@unocss/reset": "^0.52.3",
    "@vueuse/core": "^10.1.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@iconify-json/teenyicons": "^1.1.5",
    "@nuxt/eslint-config": "^0.1.1",
    "@types/node": "^20.2.3",
    "@unocss/eslint-config": "^0.52.3",
    "@unocss/preset-wind": "^0.52.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "lint-staged": "^13.2.2",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "unocss": "^0.52.3",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.8",
    "vue-tsc": "^1.6.5"
  }
}
