{
  "name": "more-wordle",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,vue}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{css,html,json,md,ts,vue,yml}\" --ignore-path .gitignore",
    "prepare": "pnpm exec simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run typecheck"
  },
  "dependencies": {
    "@unocss/reset": "^0.37.0",
    "@vueuse/core": "^8.5.0",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@iconify-json/teenyicons": "^1.1.1",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@unocss/preset-typography": "^0.37.0",
    "@unocss/preset-wind": "^0.37.0",
    "@unocss/transformer-directives": "^0.37.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.0.1",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.7.2",
    "unocss": "^0.37.0",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.9",
    "vue-tsc": "^0.35.2"
  }
}
