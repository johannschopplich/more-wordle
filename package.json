{
  "name": "more-wordle",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@7.14.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:types": "vue-tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,vue}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{css,html,json,md,ts,vue,yml}\" --ignore-path .gitignore",
    "prepare": "simple-git-hooks"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --cache --ignore-path .gitignore"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "dependencies": {
    "@leanera/vue-i18n": "^0.3.10",
    "@unocss/reset": "^0.46.1",
    "@vueuse/core": "^9.4.0",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@iconify-json/teenyicons": "^1.1.3",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@unocss/preset-wind": "^0.46.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-vue": "^9.6.0",
    "lint-staged": "^13.0.3",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.8.4",
    "unocss": "^0.46.1",
    "unplugin-icons": "^0.14.12",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.0",
    "vue-tsc": "^1.0.9"
  }
}
