{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:types": "vue-tsc --noEmit",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{css,html,json,md,ts,vue,yml}\"",
    "prepare": "simple-git-hooks"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": "eslint --cache --ignore-path .gitignore"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@leanera/vue-i18n": "^0.5.0",
    "@unocss/reset": "^0.55.0",
    "@vueuse/core": "^10.3.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@iconify-json/teenyicons": "^1.1.6",
    "@nuxt/eslint-config": "^0.1.1",
    "@types/node": "^20.4.10",
    "@unocss/eslint-config": "^0.55.0",
    "@unocss/preset-wind": "^0.55.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.1.6",
    "unocss": "^0.55.0",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-vue-devtools": "1.0.0-rc.2",
    "vue-tsc": "^1.8.8"
  }
}
