{
  "name": "vite-wordle",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,vue}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{css,html,json,md,ts,vue,yml}\" --ignore-path .gitignore",
    "prepare": "pnpm exec simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run typecheck"
  },
  "dependencies": {
    "@unocss/reset": "^0.30.11",
    "@vueuse/core": "^8.2.4",
    "turbo-query": "^1.5.0",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@iconify-json/teenyicons": "^1.1.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@unocss/preset-typography": "^0.30.11",
    "@unocss/transformer-directives": "^0.30.11",
    "@vitejs/plugin-vue": "^2.3.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.5.0",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^4.6.3",
    "unocss": "^0.30.11",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.1",
    "vue-tsc": "^0.33.9"
  }
}
