{
  "name": "more-wordle",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,vue}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{css,html,json,md,ts,vue,yml}\" --ignore-path .gitignore",
    "prepare": "pnpm exec simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run typecheck"
  },
  "dependencies": {
    "@unocss/reset": "^0.33.2",
    "@vueuse/core": "^8.4.2",
    "turbo-query": "^1.6.1",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@babel/types": "^7.17.10",
    "@iconify-json/teenyicons": "^1.1.1",
    "@types/node": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@unocss/preset-typography": "^0.33.2",
    "@unocss/preset-wind": "^0.33.2",
    "@unocss/transformer-directives": "^0.33.2",
    "@vitejs/plugin-vue": "^2.3.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.7.1",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^4.6.4",
    "unocss": "^0.33.2",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.8",
    "vue-tsc": "^0.34.12"
  }
}
