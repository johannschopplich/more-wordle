{
  "name": "more-wordle",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@7.13.4",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:types": "vue-tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,vue}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{css,html,json,md,ts,vue,yml}\" --ignore-path .gitignore",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run test:types"
  },
  "dependencies": {
    "@leanera/vue-i18n": "^0.3.9",
    "@unocss/reset": "^0.45.29",
    "@vueuse/core": "^9.3.0",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@iconify-json/teenyicons": "^1.1.3",
    "@types/node": "18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@unocss/preset-wind": "^0.45.29",
    "@vitejs/plugin-vue": "^3.1.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-vue": "^9.6.0",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.8.4",
    "unocss": "^0.45.29",
    "unplugin-icons": "^0.14.11",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.8",
    "vue-tsc": "^1.0.7"
  }
}
